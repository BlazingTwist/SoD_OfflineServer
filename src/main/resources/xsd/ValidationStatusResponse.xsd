<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="4.1.0"
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1"
        xmlns:sod="blazingtwist/sod/" targetNamespace="blazingtwist/sod/">

    <include schemaLocation="ValidationStatus.xsd"/>

    <complexType name="ValidationStatusResponse">
        <sequence>
            <element maxOccurs="1" minOccurs="1" name="i" type="xsd:int">
                <annotation>
                    <appinfo>
                        <jaxb:property name="itemID"/>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="1" name="ves" type="sod:ValidationStatus">
                <annotation>
                    <appinfo>
                        <jaxb:property name="validationStatus"/>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="1" name="vem" type="xsd:string">
                <annotation>
                    <appinfo>
                        <jaxb:property name="errorMessage"/>
                    </appinfo>
                </annotation>
            </element>
        </sequence>
    </complexType>
</schema>